<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tracker.workout.workouttracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoriesActivity">
            <intent-filter>
                <action android:name="android.intent.action.CREATE" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogWorkoutActivity" />
        <activity android:name=".WorkoutDiaryActivity" />
        <activity android:name=".ExercisesForCategoryActivity" />
        <activity android:name=".CreateWorkoutActivity" />
        <activity android:name=".LogThisWorkoutActivity"></activity>
    </application>

</manifest>